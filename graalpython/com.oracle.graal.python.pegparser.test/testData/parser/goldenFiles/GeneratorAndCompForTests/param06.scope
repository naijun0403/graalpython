ScopeEnvironment
  Scope null Module
    Flags: [HasChildWithFreeVars]
    Symbols:
      fn: [DefLocal, Local]
    Scope fn Function
        Flags: [HasChildWithFreeVars]
        Varnames: files, dirs
        Symbols:
            a: [DefLocal, Local]
            dirs: [DefParam, Use, Local]
            files: [DefParam, Cell]
      Scope listcomp Function
            Flags: [HasFreeVars, IsComprehension]
            Varnames: .0
            Symbols:
                  .0: [DefParam, Local]
                  dir: [DefLocal, Use, DefCompIter, Local]
                  file: [DefLocal, Use, DefCompIter, Local]
                  files: [Use, Free]
                  join: [Use, GlobalImplicit]